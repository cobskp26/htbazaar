<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Regist</servlet-name>
  	<display-name>Regist</display-name>
  	<description></description>
  	<servlet-class>reg.Regist</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MySQLCon</servlet-name>
  	<display-name>MySQLCon</display-name>
  	<description></description>
  	<servlet-class>dao.MySQLCon</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Input</servlet-name>
  	<display-name>Input</display-name>
  	<description></description>
  	<servlet-class>reg.Input</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Regist</servlet-name>
  	<url-pattern>/Regist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MySQLCon</servlet-name>
  	<url-pattern>/MySQLCon</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Input</servlet-name>
  	<url-pattern>/Input</url-pattern>
  </servlet-mapping>

<welcome-file-list>
<welcome-file>reg.html</welcome-file>
</welcome-file-list>

</web-app>
